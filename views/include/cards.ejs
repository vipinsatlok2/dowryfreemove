<% const className = !data.length ? "grid-cols-1" : "grid-cols-1 w- sm:grid-cols-2 lg:grid-cols-3 2xl:grid-cols-4"  %>

<section class="grid <%= className %> gap-5">
    <% if(data.length) { %>

        <% data.map(item => { %>
            <%- include("./card.ejs", { item, options, paramsUserId, userId }) %>
        <% }) %>
        
    <% } else { %>
        <%- include("./notfound.ejs") %>
    <% } %>
</section>